<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ruins7.multiplechoice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/notebook"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QmAddQuestionActivity"></activity>
        <activity android:name=".QmAddUserActivity"></activity>
        <activity android:name=".QuizTakerActivity"></activity>
        <activity android:name=".QmDelHistoryActivity"></activity>
    </application>
    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="24" />
</manifest>